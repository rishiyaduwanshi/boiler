    {
        "name": "Boiler",
        "version": "0.0.0",
        "author": "Abhinav Prakash",
        "github": "github.com/rishiyaduwanshi/boiler",
        "description": "A CLI tool to manage reusable code snippets and stacks across multiple programming languages, with versioning, aliasing, and global store support.",
        "defaultEditor": "vim",
        "registry": "https://rishiyaduwanshi/boiler/store",
        "paths": {
            "root": "~/.boiler",
            "store": "~/.boiler/store",
            "snippets": "~/.boiler/store/snippets",
            "stacks": "~/.boiler/store/stacks",
            "logs": "~/.boiler/logs",
            "bin": "~/.boiler/bin"
        },
        "artifacts": {
            "default": "//  ",
            "py": "#  ",
            "rb": "#  ",
            "sh": "#  ",
            "bash": "#  ",
            "ps1": "#  ",
            "html": "<!--  ",
            "htm": "<!--  ",
            "css": "/*  ",
            "sql": "--  ",
            "yml": "#  ",
            "yaml": "#  ",
            "xml": "<!--  ",
            "md": "<!--  ",
            "ahk": ";  ",
            "dockerfile": "#  ",
            "gitignore": "#  ",
            "env": "#  ",
            "toml": "#  ",
            "ini": ";  "
        },
        "aliases": {}
    }